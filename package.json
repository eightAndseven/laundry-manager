{
  "name": "laundry-manager",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . POS-manager --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"POS Manager\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Reinier Santos",
  "license": "ISC",
  "dependencies": {
    "dateformat": "^3.0.3",
    "electron": "^2.0.5",
    "electron-json-storage": "^4.1.1",
    "excel4node": "^1.5.0",
    "materialize-css": "^1.0.0-rc.2",
    "xlsx": "^0.13.3"
  },
  "devDependencies": {
    "electron-packager": "^12.1.0"
  }
}
